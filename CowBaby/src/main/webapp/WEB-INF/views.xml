	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="member_managment_list" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/pages/backstageAdmit/member_managment.jsp"></property>
